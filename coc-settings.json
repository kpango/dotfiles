{
  "coc": {
    "preferences": {
      "formatOnType": true,
      "formatOnSaveFileTypes": [
        "go",
        "nix",
        "typescript",
        "dockerfile",
        "json",
        "sh",
        "zsh",
        "bash",
        "vim",
        "eruby",
        "markdown",
        "python"
      ],
      "useQuickfixForLocations": false
    },
    "source": {
      "around.enable": false,
      "neco.enable": false,
      "buffer.disableSyntaxes": [
        "go"
      ]
    }
  },
  "list": {
    "source": {
      "grep": {
        "command": "rg",
        "args": []
      }
    }
  },
  "signature": {
    "preferShownAbove": false,
    "target": "float"
  },
  "session.directory": "~/.cache/vim/sessions",
  "diagnostic": {
    "messageTarget": "echo",
    "refreshOnInsertMode": true,
    "errorSign": "üî•",
    "warningSign": "üöß",
    "infoSign": "üí¨",
    "hintSign": "üí¨"
  },
  "git": {
    "addGBlameToBufferVar": true,
    "addedSign.hlGroup": "GitGutterAdd",
    "addedSign.text": "‚ñé",
    "changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "changeRemovedSign.text": "‚ñã",
    "changedSign.hlGroup": "GitGutterChange",
    "changedSign.text": "‚ñé",
    "removedSign.hlGroup": "GitGutterDelete",
    "removedSign.text": "‚ñè",
    "topRemovedSign.hlGroup": "GitGutterDelete",
    "topRemovedSign.text": "‚ñî"
  },
  "suggest": {
    "noselect": true,
    "preferCompleteThanJumpPlaceholder": true,
    "detailField": "abbr",
    "enablePreview": true,
    "enablePreselect": false,
    "floatEnable": true,
    "snippetIndicator": "",
    "triggerAfterInsertEnter": true,
    "echodocSupport": true,
    "timeout": 5000,
    "completionItemKindLabels": {
      "function": "Ôûî",
      "method": "Ôö¶",
      "variable": "Ôúõ",
      "constant": "Ô£ø",
      "struct": "Ô≠Ñ",
      "class": "ÔÉ®",
      "interface": "Ô©í",
      "text": "Óòí",
      "enum": "Ôêµ",
      "enumMember": "ÔÄ´",
      "module": "Ôô®",
      "color": "Óà´",
      "property": "Ô™∂",
      "field": "Ô§Ω",
      "unit": "Ôëµ",
      "file": "Ôë±",
      "value": "Ô¢£",
      "event": "Ô´ç",
      "folder": "ÔÑï",
      "keyword": "Ô¢ì",
      "snippet": "Ôôç",
      "operator": "Ô§ï",
      "reference": "Ô°∫",
      "typeParameter": "Ôâ∏",
      "default": "Ôäú"
    }
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "args": [
        "serve"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "completeUnimported": true,
        "usePlaceholders": true,
        "deepCompletion": false,
        "completionDocumentation": true,
        "fuzzyMatching": true,
        "staticcheck": false,
        "watchFileChanges": true,
        "lintTool": "golangci-lint",
        "formatTool": "goimports",
        "hoverKind": "FullDocumentation",
        "env": {
          "CGO_CFLAGS": "-g -Ofast -march=native",
          "CGO_CPPFLAGS": "-g -Ofast -march=native",
          "CGO_CXXFLAGS": "-g -Ofast -march=native",
          "CGO_FFLAGS": "-g -Ofast -march=native",
          "CGO_LDFLAGS": "-g -Ofast -march=native"
        }
      }
    },
    "nix": {
      "command": "nix-lsp",
      "filetypes": [
        "nix"
      ],
      "args": []
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh",
        "zsh",
        "bash"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "python": {
      "command": "pyls",
      "filetypes": [
        "python"
      ],
      "args": []
    },
    "efm": {
      "command": "efm-langserver",
      "args": [
        "-c",
        "$HOME/.config/nvim/efm-lsp-conf.yaml"
      ],
      "filetypes": [
        "vim",
        "eruby",
        "markdown"
      ]
    }
  }
}
