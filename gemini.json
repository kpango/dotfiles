{
  "selectedAuthType": "gemini-api-key",
  "model": "gemini-2.5-pro",
  "theme": "GitHub",
  "checkpointing": true,
  "mcpServers": {
    "serena": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "256",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-v", "${PWD}:/workspace:ro",
        "-v", "${HOME}/.serena:/workspace/.serena:rw",
        "-w", "/workspace",
        "ghcr.io/oraios/serena:latest",
        "serena-mcp-server", "--transport", "stdio", "--context", "ide-assistant", "--enable-web-dashboard", "false", "--project", "/workspace"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "k8s-native": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "256",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-v ${HOME}/.kube:/root/.kube:ro",
        "-e KUBECONFIG=/root/.kube/",
        "quay.io/manusa/kubernetes_mcp_server:latest"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "k8s-cli": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "256",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-v ${HOME}/.kube:/root/.kube:ro",
        "-e KUBECONFIG=/root/.kube/",
        "-e MASK_SECRETS=true",
        "mcp/kubernetes:latest"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "cipher": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "512",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-v", "${PWD}:/workspace:ro",
        "-v", "${HOME}/.local/share/cipher:/data:rw",
        "-w", "/workspace",
        "-e GEMINI_API_KEY",
        "-e OPENAI_API_KEY",
        "-e ANTHROPIC_API_KEY",
        "ghcr.io/campfirein/cipher:latest",
        "cipher",
        "--mode", "mcp",
        "--transport", "stdio"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD",
        "GEMINI_API_KEY": "$GEMINI_API_KEY",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "ANTHROPIC_API_KEY": "$ANTHROPIC_API_KEY"
      },
      "timeout": 90000
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "128",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-e GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server:latest"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD",
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_PERSONAL_ACCESS_TOKEN"
      },
      "timeout": 60000
    },
    "slack": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull=missing",
        "--cap-drop=ALL",
        "--security-opt", "no-new-privileges",
        "--pids-limit", "128",
        "--read-only",
        "--network", "host",
        "--tmpfs", "/tmp:rw,noexec,nosuid,nodev",
        "-e SLACK_BOT_TOKEN",
        "-e SLACK_TEAM_ID",
        "-e SLACK_CHANNEL_IDS",
        "mcp/slack:latest"
      ],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD",
        "SLACK_BOT_TOKEN": "$SLACK_BOT_TOKEN",
        "SLACK_TEAM_ID": "$SLACK_TEAM_ID",
        "SLACK_CHANNEL_IDS": "$SLACK_CHANNEL_IDS"
      },
      "timeout": 60000
    },
    "lsp-go": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "gopls"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-rust": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "rust-analyzer"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-ts": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "typescript-language-server", "--", "--stdio"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-python": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "pyright-langserver", "--", "--stdio"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-cpp": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "clangd", "--", "--compile-commands-dir=${PWD}/build"],      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-zig": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "zls"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    },
    "lsp-nim": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${PWD}", "--lsp", "nimlangserver"],
      "env": {
        "HOME": "$HOME",
        "PWD": "$PWD"
      },
      "timeout": 60000
    }
  }
}
