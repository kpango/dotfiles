{
  "runtimes": {
    "runsc": {
      "path": "/usr/local/bin/runsc"
    }
  },
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3"
  },
  "debug": false,
  "live-restore": true,
  "experimental": true,
  "features": {
    "buildkit": true
  },
  "storage-driver": "overlay2",
  "default-ulimits": {
      "memlock": {
          "name": "memlock",
          "hard": -1,
          "soft": -1
      }
  },
  "default-address-pools": [
    {
      "base": "192.168.0.0/16",
      "size": 24
    }
  ]
}
